  <DataSets>
    <DataSet Name="EmployeeList">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        '&lt;a href=" ' + e.LoginID + '"&gt;' + p.LastName + ',' + p.FirstName + '&lt;/a&gt;' AS EmployeeTag, p.LastName + ',' + p.FirstName AS FullName, YEAR(e.HireDate) AS YearOfHire
FROM            HumanResources.Employee AS e INNER JOIN
                         Person.Person AS p ON e.BusinessEntityID = p.BusinessEntityID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EmployeeTag">
          <DataField>EmployeeTag</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearOfHire">
          <DataField>YearOfHire</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>

porting/reportdesigner">5005e5b5-6ff1-40f8-879a-ce587ae2a2a9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="AdvWorkDataSet"><Query><DataSourceName>AdventureWorks</DataSourceName><CommandText>SELECT        pro.Name AS Product, SUM(sod.OrderQty) AS [Total Order], DATEPART(yyyy, soh.OrderDate) AS [Order Year]
FROM            Production.Product AS pro INNER JOIN
                         Purchasing.ProductVendor AS pv ON pro.ProductID = pv.ProductID INNER JOIN
                         Sales.SalesOrderDetail AS sod ON pro.ProductID = sod.ProductID INNER JOIN
                         Sales.SalesOrderHeader AS soh ON sod.SalesOrderID = soh.SalesOrderID
GROUP BY pro.Name, DATEPART(yyyy, soh.OrderDate)
ORDER BY [Order Year]</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Product"><DataField>Product</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Total_Order"><DataField>Total Order</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Order_Year"><DataField>Order Year</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">ad46b0bf-3f0b-4c9e-a37f-d6f31f4f6d0a</rd:ReportID></Report>
FROM            Sales.SalesOrderHeader AS soh INNER JOIN
                         Sales.SalesOrderDetail AS sod ON soh.SalesOrderID = sod.SalesOrderID INNER JOIN
                         Person.BusinessEntity AS be ON soh.SalesPersonID = be.BusinessEntityID INNER JOIN
                         Person.Person AS p ON be.BusinessEntityID = p.BusinessEntityID
GROUP BY p.FirstName, p.MiddleName, p.LastName
ORDER BY [Total Order]</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Sales_Person_Name"><DataField>Sales Person Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Total_Order"><DataField>Total Order</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">14aeb173-a25d-4a8d-bba7-9bc426dc5691</rd:ReportID></Report>